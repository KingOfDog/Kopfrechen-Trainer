      <TilePane.margin>
        <Insets top="-175.0" />
      </TilePane.margin>
    </Button>
    <Label id="labelTitle" alignment="TOP_CENTER" contentDisplay="CENTER" prefWidth="600.0" text="Deine Statistiken" textAlignment="CENTER" TilePane.alignment="TOP_CENTER">
