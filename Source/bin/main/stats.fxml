        <Font size="20.0" />
      </font>
      <TilePane.margin>
        <Insets top="-150.0" fx:id="x1" />
      </TilePane.margin>
    </Label>
    <Label fx:id="exercises" text="Aufgaben: " TilePane.alignment="CENTER_LEFT">
      <TilePane.margin>
        <Insets top="-200.0" />
      </TilePane.margin>
    </Label>
    <Label fx:id="exercisesSolved" text="Gelöste Aufgaben:" TilePane.alignment="CENTER_LEFT" TilePane.margin="$x1" />
